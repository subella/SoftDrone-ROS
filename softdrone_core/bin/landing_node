#!/usr/bin/env python
"""Script that uses position control to fly waypoints."""
import softdrone_ros
import rospy


def main():
    """Run everything."""
    rospy.init_node("landing_node")

    state_machine = softdrone_ros.LandingStateMachine()  # NOQA

    rospy.spin()


if __name__ == "__main__":
    main()
